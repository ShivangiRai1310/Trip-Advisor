                <ul class="navbar-nav ml-auto navbar-list">
                    <li class="nav-item"><a class="nav-link mt-2" href="user-profile">Profile</a></li>
                    <li class="nav-item"><a class="nav-link mt-2" href="logout">Logout</a></li>
                    <li><button type="button" name="sign-in" class="btn btn-dark btn-lg px-1" id="sign-in-button"
                            style="background-color: #000839;">
                            <i class="fas fa-user-circle"></i>
                        </button></li>
                </ul>
            </div>
        </nav>

        </header>
 
   <!-- PROFILE -->
   <section id="profile">
    <div class="container-fluid pt-md-5 mt-md-1">
        <h2 class="text-muted text-center mb-4 pb-4">Profile</h2>
        <div class="row">

          <form action="" method="post" class="mx-auto">
              
              <div class="py-2">
                <label>User Id</label>
                <p><%= user.user_id %></p>
              </div>
              <div class="form-group py-2">
                <label>User Name</label>
                <p><%= user.user_name %></p>
                <input type="text" class="form-control" placeholder="Enter User Name">
              </div>
              <div class="form-group py-2">
                <label>Contact</label>
                <p><%= user.contact %></p>
                <input type="tel" class="form-control" placeholder="Enter contact details">
              </div>
              <div class="form-group py-2">
                <label>Email Address</label>
                <p><%= user.email %></p>
                <input type="email" class="form-control" placeholder="abc@example.com">
              </div>
              <div class="form-group py-2">
                <label for="user-password">Password</label>
                <p><%= user.password %></p>
                <input type="password" class="form-control" id="user-password" placeholder="Enter password">
              </div>
              <div class="py-2">
                <label>Registration Date</label>
                <p><%= user.reg_date %></p>
              </div>
              <div class="py-2">
                <label>Last Updation Date</label>
                <p><%= user.updation_date %></p>
              </div>
                <button type="button" class="btn btn-info btn-lg mb-5" id="edit-profile"><i class="fas fa-edit fa-lg mr-2"></i>Edit Profile</button>
                <button type="submit" class="btn btn-success btn-lg mb-5"><i class="fas fa-user-edit fa-lg mr-2"></i>Update</button>
          </form>


        </div>
    </div>
  </section>


  <!--FOOTER -->
  <footer>
    <div class="container-fluid">
          <div class="row border-top pt-3">

            <div class="col-lg-6 text-center">
              <ul class="list-inline">
                <li class="list-inline-item mr-2">
                  <a href="#" class="text-dark">Trip Advisor</a>
                </li>
                <li class="list-inline-item mr-2">
                  <a href="#" class="text-dark">About</a>
                </li>
                <li class="list-inline-item mr-2">
                  <a href="#" class="text-dark">Support</a>
                </li>
                <li class="list-inline-item mr-2">
                  <a href="#" class="text-dark">Blog</a>
                </li>
              </ul>
            </div>

            <div class="col-lg-6 text-center">
              <p>&copy; 2020 Copyright. Made With <i class="fas fa-heart text-danger"></i> by <span class="text-success">Trip Advisor</span> </p>
            </div>

          </div>
    </div>
  </footer>


